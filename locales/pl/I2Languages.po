msgid "Free Camera"
msgstr "Свободная камера"

msgctxt "Options Screen/KEY_OPTIONS_FREE_CAMERA_NAME"
msgid "FREE CAMERA"
msgstr "СВОБОДНАЯ КАМЕРА"

msgctxt "Options Screen/KEY_OPTIONS_GAMEPAD_DESCRIPTION"
