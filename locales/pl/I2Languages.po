msgid "x {0} shots"
msgstr "x {0} выстрелов"

msgctxt "Tactical/KEY_OUT_OF_AMMO__RELOAD"
msgid "OUT OF AMMO - RELOAD"
msgstr "НЕТ БОЕПРИПАСОВ — ПЕРЕЗАРЯДИТЕ"

