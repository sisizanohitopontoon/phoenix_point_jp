msgctxt "Abilities/KEY_SIREN_SLASHING_STRIKE_NAME"
msgid "SLASHING STRIKE"
msgstr "РЕЖУЩИЙ УДАР"

msgctxt "Abilities/KEY_SIREN_VOMIT_ACID_DESCRIPTION"
