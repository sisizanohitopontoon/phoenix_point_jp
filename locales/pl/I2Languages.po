msgstr "ПОГЛОЩЕНИЕ"

msgctxt "Abilities/KEY_PDWS_TALENT_DESCRIPTION"
msgid "Proficiency with PDWs at +10% Accuracy"
msgstr "+10% к точности при использования оружия самообороны."

msgctxt "Abilities/KEY_PDWS_TALENT_NAME"
