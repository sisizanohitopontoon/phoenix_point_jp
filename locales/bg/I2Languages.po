msgctxt "Blood Titanium/KEY_NJ_EXO_BIO_TORSO_NAME2"
msgid "Neural Torso"
msgstr "神经躯干"

msgctxt "Blood Titanium/KEY_NJ_JUGG_BIO_HELMET_DESCRIPTION"
