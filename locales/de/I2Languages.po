msgstr "Tutti i nemici organici entro 15 caselle vengono rivelati automaticamente."

msgctxt "Abilities/KEY_MUTOG_MIND_SENSE_NAME"
msgid "MIND SENSE"
msgstr "PERCEZIONE MENTALE"

msgctxt "Abilities/KEY_MUTOG_PAW_BASH_DESCRIPTION"
