msgstr "Immunità ai danni da veleno."

msgctxt "Abilities/KEY_POISON_IMMUNITY_NAME"
msgid "POISON IMMUNITY"
msgstr "IMMUNITÀ AL VELENO"

msgctxt "Abilities/KEY_POWER_SURGE_DESCRIPTION"
