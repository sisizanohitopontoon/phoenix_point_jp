msgid "ACID ABDOMEN"
msgstr "ADDOME ACIDO"

msgctxt "Blood Titanium/KEY_CHIRON_TORSO_ACID_MORTAR_DESCRIPTION"
msgid "ACID ABDOMEN"
msgstr "ADDOME ACIDO"

