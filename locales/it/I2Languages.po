msgid "ACID LAUNCHER ARM"
msgstr "BRACCIO LANCIATORE ACIDO"

msgctxt "Blood Titanium/KEY_ARTHRON_ACID_GRENADE_ARM_DESCRIPTION"
msgid "Organic Grenade Launcher"
msgstr "Lanciagranate organico"

