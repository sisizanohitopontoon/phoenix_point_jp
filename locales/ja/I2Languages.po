msgctxt "Blood Titanium/KEY_NJ_EXO_BIO_LEGS_NAME2"
msgid "Propeller Legs"
msgstr "プロペラ レッグ"

msgctxt "Blood Titanium/KEY_NJ_EXO_BIO_TORSO_DESCRIPTION"
